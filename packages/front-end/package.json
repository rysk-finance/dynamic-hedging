    "react-toastify": "^9.1.1",
    "react-tooltip": "^4.5.1",
    "recharts": "^2.5.0",
    "use-count-up": "^3.0.1",
    "use-debounce": "^9.0.3",
    "wagmi": "^0.12.2",
    "web-vitals": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.x.x",
    "@types/react": "^18.0.28",
