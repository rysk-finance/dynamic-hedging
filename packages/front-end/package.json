{
  "name": "front-end",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "react-app-rewired build && yarn sourcemaps:inject && yarn sourcemaps:upload",
    "eject": "react-app-rewired eject",
    "sourcemaps:inject": "sentry-cli sourcemaps inject ./build/static/js",
    "sourcemaps:upload": "node ./upload-sourcemaps.js",
    "start": "env-cmd -f .env -f .env.local react-app-rewired start",
    "start:arbitrum": "env-cmd -f .env -f .env.arbitrum react-app-rewired start",
    "start:testnet": "env-cmd -f .env -f .env.testnet react-app-rewired start",
    "test": "react-app-rewired test"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@rainbow-me/rainbowkit": "^0.12.1",
    "@ramonak/react-progress-bar": "^5.0.3",
    "@sentry/cli": "^2.17.5",
    "@sentry/react": "^7.43.0",
    "@unstoppabledomains/resolution": "^8.5.0",
    "fathom-client": "^3.5.0",
    "framer-motion": "^10.6.1",
    "graphql": "^16.6.0",
    "hex-to-css-filter": "^5.4.0",
    "implied-volatility": "^1.0.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-content-loader": "^6.2.1",
    "react-countup": "^6.4.1",
    "react-datepicker": "^4.10.0",
    "react-dom": "^18.2.0",
