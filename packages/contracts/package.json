	"keywords": [],
	"main": "index.js",
	"workspaces": {
		"nohoist": [
			"**/typechain",
			"**/typechain/**",
			"**/@typechain",
			"**/@typechain/**"
		]
	},
	"scripts": {
		"buyoption:testnet": "npx hardhat run scripts/buySample.ts --network arbitrumGoerli",
		"compile": "hardhat clean && hardhat compile && hardhat typechain && ./copyABIsToFrontend.sh",
		"compile:ci": "hardhat clean && hardhat compile && hardhat typechain",
		"create-otc-order:testnet": "npx hardhat run scripts/createOTCOrder.ts --network arbitrumGoerli",
		"mint-weth:testnet": "npx hardhat run scripts/frontend-scripts/mintWETH.ts --network arbitrumGoerli",
		"create-otc-strangle:testnet": "npx hardhat run scripts/createOTCStrangle.ts --network arbitrumGoerli",
		"deploy:arbitrumGoerli": "npx hardhat run deploy/arbitrum-goerli/ArbitrumGoerliDeploy.ts --network arbitrumGoerli",
		"deploy:localhost": "npx hardhat deploy --network localhost --tags localhost && ./copyAddressesToFrontEnd.sh --deploy-scripts deploy/localhost",
		"deployAlpha:arbitrumGoerli": "npx hardhat run deploy/arbitrum-goerli/ArbitrumGoerliAlphaDeploy.ts --network arbitrumGoerli",
		"deployAlpha:mainnet": "npx hardhat run deploy/arbitrum/ArbitrumAlphaDeploy.ts --network arbitrum",
		"deployRangeOrderReactor:arbitrum": "npx hardhat run deploy/arbitrum/DeployRangeOrderReactor.ts --network arbitrum",
		"deployRangeOrderReactor:arbitrumGoerli": "npx hardhat run deploy/arbitrum/DeployRangeOrderReactor.ts --network arbitrumGoerli",
		"deployOpynUpgrade:arbitrumGoerli": "npx hardhat run deploy/arbitrum/deployOpynSpreadUpgrade.ts --network arbitrumGoerli",
		"deployOpynUpgrade:arbitrum": "hardhat run deploy/arbitrum/deployOpynSpreadUpgrade.ts --network arbitrum",
		"deployLens:arbitrum": "npx hardhat run deploy/arbitrum/deployDHVLensMK1.ts --network arbitrum",
		"deployQuantLens:arbitrum": "npx hardhat run deploy/arbitrum/deployQuantPositionLensMK1.ts --network arbitrum",
		"deployLens:arbitrumGoerli": "npx hardhat run deploy/arbitrum/deployDHVLensMK1.ts --network arbitrumGoerli",
		"deployUserLens:arbitrum": "npx hardhat run deploy/arbitrum/deployUserPositionLensMK1.ts --network arbitrum",
		"deployUserLens:arbitrumGoerli": "npx hardhat run deploy/arbitrum/deployUserPositionLensMK1.ts --network arbitrumGoerli",
		"execute-epoch:testnet": "npx hardhat run scripts/executeEpochTestnet.ts --network arbitrumGoerli",
		"issue-series:testnet": "npx hardhat run scripts/issue-series.ts --network arbitrumGoerli",
		"issue-series:mainnet": "npx hardhat run scripts/issue-series.ts --network arbitrum",
		"change-dhv-pos:mainnet": "npx hardhat run scripts/change-dhv-pos.ts --network arbitrum",
		"set-vals:testnet": "npx hardhat run scripts/set-vals.ts --network arbitrumGoerli",
		"set-collat-params:testnet": "npx hardhat run scripts/set-collat-params.ts --network arbitrumGoerli",
		"fulfill:testnet": "npx hardhat run scripts/fulfill.ts --network arbitrumGoerli",
		"hardhat-run": "hardhat run",
		"test": "hardhat test",
		"test-coverage": "export NODE_OPTIONS='--max-old-space-size=8192' && hardhat coverage --testfiles 'test/*.ts'"
	},
	"dependencies": {
		"@chainlink/contracts": "0.6.1",
		"@openzeppelin/contracts": "4.9.6",
		"@rage/core": "https://github.com/RageTrade/core.git",
